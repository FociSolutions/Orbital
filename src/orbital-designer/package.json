{
  "name": "orbital-designer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --configuration production --source-map=false",
    "format": "prettier --write .",
    "format:ci": "prettier --check \"*.{json,yml,yaml,md,css}\"",
    "test": "jest",
    "test:watch": "jest -o --watch",
    "test:watchAll": "jest --watchAll",
    "test:dashboard": "jest --watch --reporters=jest-dashboard",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --config=jest.config.js",
    "lint": "eslint 'src/**/*.{ts,html}' --cache --report-unused-disable-directives --fix",
    "lint-f": "eslint 'src/**/*.{ts,html}' --report-unused-disable-directives --fix",
    "lint:ci": "eslint 'src/**/*.{ts,html}'",
    "lint-ng": "ng lint",
    "type": "tsc --noEmit -p tsconfig.app.json",
    "type:spec": "tsc --noEmit -p tsconfig.spec.json",
    "e2e": "ng e2e",
    "sb": "sb",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "cd ../.. && husky install ./src/orbital-designer/.husky",
    "pre-commit": "lint-staged",
    "pkg-chk": "package-lock-check"
  },
  "lint-staged": {
    "*.{js,json,yaml,yml,html,css,scss,md}": "prettier --write",
    "*.ts": "eslint --cache --fix"
  },
  "dependencies": {
    "@angular/animations": "~13.3.7",
    "@angular/cdk": "~13.3.7",
    "@angular/common": "~13.3.7",
    "@angular/compiler": "~13.3.7",
    "@angular/core": "~13.3.7",
    "@angular/forms": "~13.3.7",
    "@angular/material": "~13.3.7",
    "@angular/platform-browser": "~13.3.7",
    "@angular/platform-browser-dynamic": "~13.3.7",
    "@angular/router": "~13.3.7",
    "ang-jsoneditor": "^1.10.5",
    "deep-equal": "^2.0.5",
    "file-saver": "^2.0.5",
    "http-status-codes": "^2.2.0",
    "js-yaml": "^4.1.0",
    "jsoneditor": "^9.7.4",
    "jsonpath": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "ngx-logger": "^5.0.9",
    "openapi-schema-validator": "^11.0.0",
    "openapi-types": "^11.0.0",
    "prettier": "^2.6.2",
    "rxjs": "~7.5.5",
    "rxjs-observable-store": "^3.1.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.4",
    "@angular-devkit/architect": "^0.1303.5",
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.7",
    "@angular/elements": "^13.3.7",
    "@angular/language-service": "~13.3.7",
    "@babel/core": "^7.17.10",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/angular": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/cli": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@types/faker": "^6.6.9",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@webcomponents/custom-elements": "^1.5.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jasmine-core": "^4.5.0",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^27.5.1",
    "jest-dashboard": "^0.3.1",
    "jest-junit": "^13.2.0",
    "jest-preset-angular": "^11.1.2",
    "lint-staged": "^13.1.2",
    "package-lock-check": "^1.1.0",
    "protractor": "~7.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.6.4"
  }
}
