{
  "name": "orbital-designer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --configuration production --source-map=false",
    "format": "prettier --write .",
    "format:ci": "prettier --check \"*.{json,yml,yaml,md,css}\"",
    "test": "jest",
    "test:watch": "jest -o --watch",
    "test:watchAll": "jest --watchAll",
    "test:dashboard": "jest --watch --reporters=jest-dashboard",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --config=jest.config.js",
    "lint": "eslint 'src/**/*.{ts,html}' --cache --report-unused-disable-directives --fix",
    "lint-f": "eslint 'src/**/*.{ts,html}' --report-unused-disable-directives --fix",
    "lint:ci": "eslint 'src/**/*.{ts,html}'",
    "lint-ng": "ng lint",
    "type": "tsc --noEmit -p tsconfig.app.json",
    "type:spec": "tsc --noEmit -p tsconfig.spec.json",
    "e2e": "ng e2e",
    "sb": "sb",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "cd ../.. && husky install ./src/orbital-designer/.husky",
    "pre-commit": "lint-staged",
    "pkg-chk": "package-lock-check"
  },
  "lint-staged": {
    "*.{js,json,yaml,yml,html,css,scss,md}": "prettier --write",
    "*.ts": "eslint --cache --fix"
  },
  "dependencies": {
    "@angular/animations": "~15.2.0",
    "@angular/cdk": "^15.2.1",
    "@angular/common": "~15.2.0",
    "@angular/compiler": "~15.2.0",
    "@angular/core": "~15.2.0",
    "@angular/forms": "~15.2.0",
    "@angular/material": "^15.2.1",
    "@angular/platform-browser": "~15.2.0",
    "@angular/platform-browser-dynamic": "~15.2.0",
    "@angular/router": "~15.2.0",
    "ang-jsoneditor": "^1.10.5",
    "deep-equal": "^2.2.0",
    "file-saver": "^2.0.5",
    "http-status-codes": "^2.2.0",
    "js-yaml": "^4.1.0",
    "jsoneditor": "^9.10.0",
    "jsonpath": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "ngx-logger": "^5.0.11",
    "openapi-schema-validator": "^12.1.0",
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.4",
    "rxjs": "~7.8.0",
    "rxjs-observable-store": "^3.1.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/architect": "^0.1502.0",
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "~15.2.0",
    "@angular/elements": "^15.2.0",
    "@angular/language-service": "~15.2.0",
    "@babel/core": "^7.21.0",
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/angular": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.14.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@webcomponents/custom-elements": "^1.5.1",
    "babel-loader": "^9.1.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "^4.5.0",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.4.3",
    "jest-dashboard": "^0.3.1",
    "jest-junit": "^15.0.0",
    "jest-preset-angular": "^13.0.0",
    "lint-staged": "^13.1.2",
    "package-lock-check": "^1.1.0",
    "protractor": "~7.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  }
}
